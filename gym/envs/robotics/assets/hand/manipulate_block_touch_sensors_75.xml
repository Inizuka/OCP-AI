<?xml version="1.0" encoding="utf-8"?>
<mujoco>
    <compiler angle="radian" coordinate="local" meshdir="../stls/hand" texturedir="../textures"></compiler>
    <option timestep="0.002" iterations="20" apirate="200">
        <flag warmstart="enable"></flag>
    </option>

    <include file="shared.xml"></include>

    <asset>
        <include file="shared_asset.xml"></include>

        <texture name="texture:object" file="block.png" gridsize="3 4" gridlayout=".U..LFRB.D.."></texture>
        <texture name="texture:hidden" file="block_hidden.png" gridsize="3 4" gridlayout=".U..LFRB.D.."></texture>

        <material name="material:object" texture="texture:object" specular="1" shininess="0.3" reflectance="0"></material>
        <material name="material:hidden" texture="texture:hidden" specular="1" shininess="0.3" reflectance="0"></material>
        <material name="material:target" texture="texture:object" specular="1" shininess="0.3" reflectance="0" rgba="1 1 1 0.5"></material>
    </asset>

    <worldbody>
        <geom name="floor0" pos="1 1 0" size="1 1 1" type="plane" condim="3" material="floor_mat"></geom>
        <body name="floor0" pos="1 1 0"></body>

        <include file="robot_touch_sensors_75.xml"></include>

        <body name="object" pos="1 0.87 0.2">
            <geom name="object" type="box" size="0.025 0.025 0.025" material="material:object" condim="4" density="567"></geom>
            <geom name="object_hidden" type="box" size="0.024 0.024 0.024" material="material:hidden" condim="4" contype="0" conaffinity="0" mass="0"></geom>
            <site name="object:center" pos="0 0 0" rgba="1 0 0 0" size="0.01 0.01 0.01"></site>
            <joint name="object:joint" type="free" damping="0.01"></joint>
        </body>
        <body name="target" pos="1 0.87 0.2">
            <geom name="target" type="box" size="0.025 0.025 0.025" material="material:target" condim="4" group="2" contype="0" conaffinity="0"></geom>
            <site name="target:center" pos="0 0 0" rgba="1 0 0 0" size="0.01 0.01 0.01"></site>
            <joint name="target:joint" type="free" damping="0.01"></joint>
        </body>

        <light directional="true" ambient="0.2 0.2 0.2" diffuse="0.8 0.8 0.8" specular="0.3 0.3 0.3" castshadow="false" pos="0 1 4" dir="0 0 -1" name="light0"></light>
    </worldbody>

    <sensor>

        <!--THUMB-->
        <touch name="robot0:TS_thtip_tip" site="robot0:T_thtip_tip"></touch>
        <touch name="robot0:TS_thtip_front" site="robot0:T_thtip_front"></touch>
        <touch name="robot0:TS_thtip_back" site="robot0:T_thtip_back"></touch>
        <touch name="robot0:TS_thtip_left" site="robot0:T_thtip_left"></touch>
        <touch name="robot0:TS_thtip_right" site="robot0:T_thtip_right"></touch>

        <touch name="robot0:TS_thmiddle_front" site="robot0:T_thmiddle_front"></touch>
        <touch name="robot0:TS_thmiddle_back" site="robot0:T_thmiddle_back"></touch>
        <touch name="robot0:TS_thmiddle_left" site="robot0:T_thmiddle_left"></touch>
        <touch name="robot0:TS_thmiddle_right" site="robot0:T_thmiddle_right"></touch>

        <touch name="robot0:TS_thproximal_front" site="robot0:T_thproximal_front"></touch>
        <touch name="robot0:TS_thproximal_back" site="robot0:T_thproximal_back"></touch>
        <touch name="robot0:TS_thproximal_left" site="robot0:T_thproximal_left"></touch>
        <touch name="robot0:TS_thproximal_right" site="robot0:T_thproximal_right"></touch>

        <!-- LEFT FINGER -->
        <touch name="robot0:TS_lftip_tip" site="robot0:T_lftip_tip"></touch>
        <touch name="robot0:TS_lftip_front" site="robot0:T_lftip_front"></touch>
        <touch name="robot0:TS_lftip_back" site="robot0:T_lftip_back"></touch>
        <touch name="robot0:TS_lftip_left" site="robot0:T_lftip_left"></touch>
        <touch name="robot0:TS_lftip_right" site="robot0:T_lftip_right"></touch>

        <touch name="robot0:TS_lfmiddle_front" site="robot0:T_lfmiddle_front"></touch>
        <touch name="robot0:TS_lfmiddle_back" site="robot0:T_lfmiddle_back"></touch>
        <touch name="robot0:TS_lfmiddle_left" site="robot0:T_lfmiddle_left"></touch>
        <touch name="robot0:TS_lfmiddle_right" site="robot0:T_lfmiddle_right"></touch>

        <touch name="robot0:TS_lfproximal_front_d" site="robot0:T_lfproximal_front_d"></touch>
        <touch name="robot0:TS_lfproximal_front_u" site="robot0:T_lfproximal_front_u"></touch>
        <touch name="robot0:TS_lfproximal_back" site="robot0:T_lfproximal_back"></touch>
        <touch name="robot0:TS_lfproximal_left" site="robot0:T_lfproximal_left"></touch>
        <touch name="robot0:TS_lfproximal_right" site="robot0:T_lfproximal_right"></touch>

        <!-- RIGHT FINGER -->
        <touch name="robot0:TS_rftip_tip" site="robot0:T_rftip_tip"></touch>
        <touch name="robot0:TS_rftip_front" site="robot0:T_rftip_front"></touch>
        <touch name="robot0:TS_rftip_back" site="robot0:T_rftip_back"></touch>
        <touch name="robot0:TS_rftip_left" site="robot0:T_rftip_left"></touch>
        <touch name="robot0:TS_rftip_right" site="robot0:T_rftip_right"></touch>

        <touch name="robot0:TS_rfmiddle_front" site="robot0:T_rfmiddle_front"></touch>
        <touch name="robot0:TS_rfmiddle_back" site="robot0:T_rfmiddle_back"></touch>
        <touch name="robot0:TS_rfmiddle_left" site="robot0:T_rfmiddle_left"></touch>
        <touch name="robot0:TS_rfmiddle_right" site="robot0:T_rfmiddle_right"></touch>

        <touch name="robot0:TS_rfproximal_front_d" site="robot0:T_rfproximal_front_d"></touch>
        <touch name="robot0:TS_rfproximal_front_u" site="robot0:T_rfproximal_front_u"></touch>
        <touch name="robot0:TS_rfproximal_back" site="robot0:T_rfproximal_back"></touch>
        <touch name="robot0:TS_rfproximal_left" site="robot0:T_rfproximal_left"></touch>
        <touch name="robot0:TS_rfproximal_right" site="robot0:T_rfproximal_right"></touch>

        <!-- MIDDLE FINGER -->
        <touch name="robot0:TS_mftip_tip" site="robot0:T_mftip_tip"></touch>
        <touch name="robot0:TS_mftip_front" site="robot0:T_mftip_front"></touch>
        <touch name="robot0:TS_mftip_back" site="robot0:T_mftip_back"></touch>
        <touch name="robot0:TS_mftip_left" site="robot0:T_mftip_left"></touch>
        <touch name="robot0:TS_mftip_right" site="robot0:T_mftip_right"></touch>

        <touch name="robot0:TS_mfmiddle_front" site="robot0:T_mfmiddle_front"></touch>
        <touch name="robot0:TS_mfmiddle_back" site="robot0:T_mfmiddle_back"></touch>
        <touch name="robot0:TS_mfmiddle_left" site="robot0:T_mfmiddle_left"></touch>
        <touch name="robot0:TS_mfmiddle_right" site="robot0:T_mfmiddle_right"></touch>

        <touch name="robot0:TS_mfproximal_front_d" site="robot0:T_mfproximal_front_d"></touch>
        <touch name="robot0:TS_mfproximal_front_u" site="robot0:T_mfproximal_front_u"></touch>
        <touch name="robot0:TS_mfproximal_back" site="robot0:T_mfproximal_back"></touch>
        <touch name="robot0:TS_mfproximal_left" site="robot0:T_mfproximal_left"></touch>
        <touch name="robot0:TS_mfproximal_right" site="robot0:T_mfproximal_right"></touch>

         <!--FIFTH FINGER-->
        <touch name="robot0:TS_fftip_tip" site="robot0:T_fftip_tip"></touch>
        <touch name="robot0:TS_fftip_front" site="robot0:T_fftip_front"></touch>
        <touch name="robot0:TS_fftip_back" site="robot0:T_fftip_back"></touch>
        <touch name="robot0:TS_fftip_left" site="robot0:T_fftip_left"></touch>
        <touch name="robot0:TS_fftip_right" site="robot0:T_fftip_right"></touch>

        <touch name="robot0:TS_ffmiddle_front" site="robot0:T_ffmiddle_front"></touch>
        <touch name="robot0:TS_ffmiddle_back" site="robot0:T_ffmiddle_back"></touch>
        <touch name="robot0:TS_ffmiddle_left" site="robot0:T_ffmiddle_left"></touch>
        <touch name="robot0:TS_ffmiddle_right" site="robot0:T_ffmiddle_right"></touch>

        <touch name="robot0:TS_ffproximal_front_d" site="robot0:T_ffproximal_front_d"></touch>
        <touch name="robot0:TS_ffproximal_front_u" site="robot0:T_ffproximal_front_u"></touch>
        <touch name="robot0:TS_ffproximal_back" site="robot0:T_ffproximal_back"></touch>
        <touch name="robot0:TS_ffproximal_left" site="robot0:T_ffproximal_left"></touch>
        <touch name="robot0:TS_ffproximal_right" site="robot0:T_ffproximal_right"></touch>

        <!--PALM-->
        <touch name="robot0:TS_palm_bl" site="robot0:T_palm_bl"></touch>
        <touch name="robot0:TS_palm_bm" site="robot0:T_palm_bm"></touch>
        <touch name="robot0:TS_palm_br" site="robot0:T_palm_br"></touch>
        <touch name="robot0:TS_palm_fl" site="robot0:T_palm_fl"></touch>
        <touch name="robot0:TS_palm_fm" site="robot0:T_palm_fm"></touch>
        <touch name="robot0:TS_palm_fr" site="robot0:T_palm_fr"></touch>
    </sensor>

</mujoco>
